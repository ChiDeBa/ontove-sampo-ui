{
    "id": "Entities",
    "endpoint": {
        "url": "http://ontove-endpoint.chitontove.it/",
        "useAuth": false,
        "prefixesFile": "SparqlQueriesPrefixes.js"
    },
    "sparqlQueriesFile": "SparqlQueriesEntities.js",
    "baseURI": "https://github.com/ChiDeBa/ontove",
    "URITemplate": "<BASE_URI>/work/<LOCAL_ID>",
    "facetClass": "arco:ArchaeologicalProperty",
    "frontPageImage": "main_page/weapons.jpg",
    "searchMode": "faceted-search",
    "defaultActiveFacets": [
        "prefLabel"
    ],
    "defaultTab": "table",
    "defaultInstancePageTab": "table",
    "resultClasses": {
        "Entities": {
            "paginatedResultsConfig": {
                "tabID": 0,
                "component": "ResultTable",
                "tabPath": "table",
                "tabIcon": "CalendarViewDay",
                "propertiesQueryBlock": "EntityProperties",
                "pagesize": 10,
                "sortBy": null,
                "sortDirection": null
            },
            "instanceConfig": {
                "propertiesQueryBlock": "EntityProperties",
                "instancePageResultClasses": {
                    "instancePageTable": {
                        "tabID": 0,
                        "component": "InstancePageTable",
                        "tabPath": "table",
                        "tabIcon": "CalendarViewDay"
                    },
                    "instancePageExport": {
                        "tabID": 1,
                        "component": "Export",
                        "tabPath": "export",
                        "tabIcon": "CloudDownload",
                        "pageType": "instancePage"
                    }
                }
            }
        },
        "export": {
            "tabID": 1,
            "component": "Export",
            "tabPath": "export",
            "tabIcon": "CloudDownload",
            "resultClass": "Entities",
            "facetClass": "Entities"
        },
        "knowledgeGraphMetadata": {
            "sparqlQuery": "knowledgeGraphMetadataQuery",
            "resultMapper": "makeObjectList"
        }
    },
    "properties": [
        {
            "id": "uri",
            "valueType": "object",
            "makeLink": true,
            "externalLink": true,
            "sortValues": true,
            "numberedList": false,
            "onlyOnInstancePage": true
        },
        {
            "id": "prefLabel",
            "valueType": "object",
            "makeLink": true,
            "externalLink": true,
            "sortValues": true,
            "numberedList": false,
            "minWidth": 250
        },
        {
            "id": "CulturalScope",
            "valueType": "object",
            "makeLink": true,
            "externalLink": true,
            "sortValues": true,
            "numberedList": false,
            "minWidth": 250
        },
        {
            "id": "Chronology",
            "valueType": "object",
            "makeLink": true,
            "externalLink": true,
            "sortValues": true,
            "numberedList": false,
            "minWidth": 250
        },
        {
            "id": "CurrentPlace",
            "valueType": "object",
            "makeLink": true,
            "externalLink": true,
            "sortValues": true,
            "numberedList": false,
            "minWidth": 250
        }, {
            "id": "FindingPlace",
            "valueType": "object",
            "makeLink": true,
            "externalLink": true,
            "sortValues": true,
            "numberedList": false,
            "minWidth": 250
        },
        {
            "id": "Geolink",
            "valueType": "object",
            "makeLink": true,
            "externalLink": true,
            "sortValues": true,
            "numberedList": false,
            "minWidth": 250
        },
        {
            "id": "Resourcelink",
            "valueType": "object",
            "makeLink": true,
            "externalLink": true,
            "sortValues": true,
            "numberedList": false,
            "minWidth": 250
        },
        {
            "id": "Bibliography",
            "valueType": "object",
            "makeLink": true,
            "externalLink": true,
            "sortValues": true,
            "numberedList": false,
            "minWidth": 250
        },
        {
            "id": "Reference",
            "valueType": "object",
            "makeLink": true,
            "externalLink": true,
            "sortValues": true,
            "numberedList": false,
            "minWidth": 250
        }
    ],
    "facets": {
        "CurrentPlace": {
            "containerClass": "ten",
            "facetType": "list",
            "filterType": "uriFilter",
            "predicate": "(cidoc-crm:P54_has_current_permanent_location|cidoc-crm:P55_has_current_location)",
            "facetLabelPredicate": "rdfs:label",
            "searchField": true,
            "sortButton": true,
            "sortBy": "instanceCount",
            "sortByPredicate": "(cidoc-crm:P54_has_current_permanent_location|cidoc-crm:P55_has_current_location)/rdfs:label",
            "sortDirection": "desc"
        },
        "FindingPlace": {
            "containerClass": "ten",
            "facetType": "list",
            "filterType": "uriFilter",
            "predicate": "(cidoc-crm:P53_has_former_or_current_location|ontove:has_finding_place|context-description:hasFindingLocation)",
            "facetLabelPredicate": "rdfs:label",
            "searchField": true,
            "sortButton": true,
            "sortBy": "instanceCount",
            "sortByPredicate": "(cidoc-crm:P53_has_former_or_current_location|ontove:has_finding_place|context-description:hasFindingLocation)/rdfs:label",
            "sortDirection": "desc"
        },
        "Bibliography": {
            "containerClass": "ten",
            "facetType": "list",
            "filterType": "uriFilter",
            "predicate": "context-description:hasBibliography",
            "facetLabelPredicate": "rdfs:label",
            "searchField": true,
            "sortButton": true,
            "sortBy": "instanceCount",
            "sortByPredicate": "context-description:hasBibliography/rdfs:label",
            "sortDirection": "desc"
        }
}}
