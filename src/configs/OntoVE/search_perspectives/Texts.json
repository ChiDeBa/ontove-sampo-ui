{
    "id": "Texts",
    "endpoint": {
        "url": "http://localhost:3030/OntoVE/sparql",
        "useAuth": false,
        "prefixesFile": "SparqlQueriesPrefixes.js"
    },
    "sparqlQueriesFile": "SparqlQueriesTexts.js",
    "baseURI": "https://github.com/ChiDeBa/ontove",
    "URITemplate": "<BASE_URI>/work/<LOCAL_ID>",
    "facetClass": "bibframe:Work",
    "frontPageImage": "main_page/manuscript.jpg",
    "searchMode": "faceted-search",
    "defaultActiveFacets": [
        "prefLabel"
    ],
    "defaultTab": "table",
    "defaultInstancePageTab": "table",
    "resultClasses": {
        "Texts": {
            "paginatedResultsConfig": {
                "tabID": 0,
                "component": "ResultTable",
                "tabPath": "table",
                "tabIcon": "CalendarViewDay",
                "propertiesQueryBlock": "TextProperties",
                "pagesize": 10,
                "sortBy": null,
                "sortDirection": null
            },
            "instanceConfig": {
                "propertiesQueryBlock": "TextProperties",
                "instancePageResultClasses": {
                    "instancePageTable": {
                        "tabID": 0,
                        "component": "InstancePageTable",
                        "tabPath": "table",
                        "tabIcon": "CalendarViewDay"
                    },
                    "instancePageExport": {
                        "tabID": 1,
                        "component": "Export",
                        "tabPath": "export",
                        "tabIcon": "CloudDownload",
                        "pageType": "instancePage"
                    }
                }
            }
        },
        "export": {
            "tabID": 1,
            "component": "Export",
            "tabPath": "export",
            "tabIcon": "CloudDownload",
            "resultClass": "Texts",
            "facetClass": "Texts"
        },
        "knowledgeGraphMetadata": {
            "sparqlQuery": "knowledgeGraphMetadataQuery",
            "resultMapper": "makeObjectList"
        }
    },
    "properties": [
        {
            "id": "uri",
            "valueType": "object",
            "makeLink": true,
            "externalLink": true,
            "sortValues": true,
            "numberedList": false,
            "onlyOnInstancePage": true
        },
        {
            "id": "prefLabel",
            "valueType": "object",
            "makeLink": true,
            "externalLink": true,
            "sortValues": true,
            "numberedList": false,
            "minWidth": 250
        },
        {
            "id": "Edition",
            "valueType": "object",
            "makeLink": true,
            "externalLink": true,
            "sortValues": true,
            "numberedList": false,
            "minWidth": 250
        },
        {
            "id": "originPlace",
            "valueType": "object",
            "makeLink": false,
            "externalLink": false,
            "sortValues": true,
            "numberedList": false,
            "minWidth": 250
        },
        {
            "id": "issuer",
            "valueType": "object",
            "makeLink": false,
            "externalLink": false,
            "sortValues": true,
            "numberedList": false,
            "minWidth": 250
        },
        {
            "id": "subject",
            "valueType": "object",
            "makeLink": false,
            "externalLink": false,
            "sortValues": true,
            "numberedList": false,
            "minWidth": 150
        },
        {
            "id": "originDate",
            "valueType": "object",
            "makeLink": false,
            "externalLink": false,
            "sortValues": true,
            "numberedList": false,
            "minWidth": 150
        }
    ],
    "facets": {
        "Edition": {
            "containerClass": "ten",
            "facetType": "list",
            "filterType": "uriFilter",
            "predicate": "bibframe:hasInstance",
            "facetLabelPredicate": "rdfs:label",
            "facetLabelFilter":"",
            "searchField": true,
            "sortButton": true,
            "sortBy": "instanceCount",
            "sortByPredicate": "bibframe:hasInstance/rdfs:label",
            "sortDirection": "desc"
        },
        "originPlace": {
            "containerClass": "ten",
            "facetType": "list",
            "filterType": "uriFilter",
            "predicate": "bibframe:originPlace",
            "facetLabelPredicate": "rdfs:label",
            "facetLabelFilter":"",
            "searchField": true,
            "sortButton": true,
            "sortBy": "instanceCount",
            "sortByPredicate": "bibframe:originPlace/rdfs:label",
            "sortDirection": "desc"
        },
        "issuer": {
            "containerClass": "ten",
            "facetType": "list",
            "filterType": "uriFilter",
            "predicate": "bibo:issuer",
            "facetLabelPredicate": "rdfs:label",
            "facetLabelFilter":"",
            "searchField": true,
            "sortButton": true,
            "sortBy": "instanceCount",
            "sortByPredicate": "bibo:issuer/rdfs:label",
            "sortDirection": "desc"
        },
        "subject": {
            "containerClass": "ten",
            "facetType": "list",
            "filterType": "uriFilter",
            "predicate": "bibframe:subject",
            "facetLabelPredicate": "rdfs:label",
            "facetLabelFilter":"",
            "searchField": true,
            "sortButton": true,
            "sortBy": "instanceCount",
            "sortByPredicate": "bibframe:subject/rdfs:label",
            "sortDirection": "desc"
        },
        "originDate": {
            "containerClass": "ten",
            "facetType": "list",
            "filterType": "uriFilter",
            "predicate": "bibframe:originDate",
            "facetLabelPredicate": "rdfs:label",
            "searchField": true,
            "sortButton": true,
            "sortBy": "instanceCount",
            "sortByPredicate": "bibframe:originDate/rdfs:label",
            "sortDirection": "desc"
        }     
}
}